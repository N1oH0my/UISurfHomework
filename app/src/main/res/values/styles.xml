<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="RVItemTypeStyle">
        <item name="android:textColor">@color/tabSelected</item>
        <item name="android:textSize">10sp</item>
        <item name="android:fontFamily">@font/googlesans_medium</item>

    </style>
    <style name="RVItemDetailsStyle">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/googlesans_regular</item>
    </style>
    <style name="RVItemShortDetailsStyle">
        <item name="android:textColor">@color/tabUnselected</item>
        <item name="android:textSize">12sp</item>
        <item name="android:fontFamily">@font/googlesans_regular</item>
    </style>
    <style name="RVItemNameStyle">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
        <item name="android:fontFamily">@font/googlesans_bold</item>
        <item name="android:layout_width">153sp</item>
        <item name="android:layout_height">23sp</item>
    </style>
    <style name="TabsStyle">
        <item name="android:textColor">@color/tabUnselected</item>
        <item name="android:textSize">18sp</item>
        <item name="android:fontFamily">@font/googlesans_bold</item>
    </style>

</resources>